;네코마타구상 2009_3_6
;製作者　eraSQ製作スレッド>>97
;
;カウンターとか촉수分の구상とかその他いろいろ
;
;…역조교ですか？　デスネ. 
;--------------------------------------------------
;EVENTTRAIN関係
;조교개시時のセリフ CFLAG 201～210を使用
;-------------------------------------------------
@EVENTTRAIN
SIF NO:TARGET != 6
	RETURN 1
SIF CFLAG:209 != 1
	RETURN 1

;-------------------------------------------------
;※カウンター行動の플래그ON／OFF　CFLAG:9
;CFLAG:9 = 1 に変更するとOFF
;-------------------------------------------------
;CFLAG:9 = 1

;애널봉사の台詞分岐でCFLAG:462を操作しています, 필요ならばその部分を削除してかまいません
;마스터の마조끼の操作, 参照をやめ, CFLAG:461を利用して分岐するように変更しています
;플래그が0の時み, 「쓰러뜨려짐중に조교を終わる」を避ければ上昇を回避できます

;-------------------------------------------------
;初조교時 CFLAG201
;-------------------------------------------------

IF GETBIT(CFLAG:201, 0) == 0
	DRAWLINE
	PRINTFORML 「うにゅ, ここはどこかニャ？」
	PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 周りを見回し, こちらに振り向いた
	PRINTFORML 「にゃふ…ボクを조교한다にゃ？できるかにゃ～？」
	PRINTFORMW 「にゃはは, たべちゃうニャ～」
	SETBIT CFLAG:201, 0
	RETURN 1

;-------------------------------------------------
;조교개시時（一回のみ） CFLAG201
;-------------------------------------------------
;반발각인Lv3
ELSEIF MARK:반발각인 == 3
	CALL K6_KOJO2_1

;굴복각인Lv1
ELSEIF GETBIT(CFLAG:201, 1) == 0 && MARK:굴복각인 == 1
	DRAWLINE
	PRINTFORML 「にゃは, 来たにゃ？」
	PRINTFORMW 「もっとキモチイイことするにゃ～」
	SETBIT CFLAG:201, 1
	RETURN 1

;굴복각인Lv2
ELSEIF GETBIT(CFLAG:201, 2) == 0 && MARK:굴복각인 == 2
	DRAWLINE
	PRINTFORML 「にゃは, 待ってたにゃ…」
	PRINTFORML 「もっと…キモチイイことするニャ…♪」
	;새드ッ気Lv2이상
	IF ABL:새드끼 >= 2
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の옷に손をかけ, 脱がそうとしてきた
	ELSE
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の姿を認めると, 옷を脱ぎ始めた
		CALL SETFLAG, "衣装全脱ぎ", TARGET
	ENDIF
	SETBIT CFLAG:201, 2
	RETURN 1

;굴복각인Lv3
ELSEIF GETBIT(CFLAG:201, 3) == 0 && MARK:굴복각인 == 3 && TALENT:연모 == 0
	DRAWLINE
	PRINTFORML 部屋に入ると, %조사처리(CALLNAME:TARGET,"는")% 息を荒くしてこちらを見つめてきた
	PRINTFORML 「うにゃあ……はやく……するにゃ…」
	PRINTFORML 「カラダが熱くて……疼くのにゃ…」
	;새드ッ気Lv3이상
	IF ABL:새드끼 >= 3
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% ベッドに넘어뜨리기, 抱きしめてきた
	ELSE
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% ベッドに横たわり, 熱く%조사처리(CALLNAME:MASTER,"를")% 見つめている
	ENDIF
	SETBIT CFLAG:201, 3
	RETURN 1

;굴복각인Lv3+연모
ELSEIF GETBIT(CFLAG:201, 4) == 0 && TALENT:연모
	DRAWLINE
	PRINTFORML 「にゃ……は♪」
	PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 瞳を潤ませてこちらを見つめている
	PRINTFORML 「…ボクと…ずっと, キモチイイことして…」
	PRINTFORML 「いっしょにいるにゃ……♪」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% そう言うと, ゆっくり%CALLNAME:MASTER%の方へ歩みよって来た
	SETBIT CFLAG:201, 4
	RETURN 1

;조수の有無を체크（いない場合は二回目以降へ飛ぶ）
ELSEIF ASSI <= 0
	CALL K6_KOJO2_1

;구상のある조수が居ない場合は, 通常の二回目以降の구상へ飛ぶ
ELSE
	CALL K6_KOJO2_1
ENDIF

;-------------------------------------------------
;조교개시時（2回目以降, @KXXX_KOJO2のXXXを캐릭터번호に変更する） CFLAGなし 
;-------------------------------------------------

@K6_KOJO2_1
;반발각인Lv3
IF MARK:반발각인 == 3
	DRAWLINE
	PRINTFORMW 「む, 何しに来たニャ？ 無駄だからさっさと帰れにゃ」
	RETURN 1
;굴복각인Lv1이하
ELSEIF MARK:굴복각인 <= 1
	DRAWLINE
	PRINTFORML 「にゃは…来たかニャ？」
	PRINTFORMW 「もっと……キモチイイことするにゃ…♪」
;굴복각인Lv2이하
ELSEIF MARK:굴복각인 <= 2
	DRAWLINE
	PRINTFORML 「にゃは…来たかニャ？」
	PRINTFORML 「もっと……キモチイイことするにゃ…♪」
	;새드ッ気Lv2이상, 역조교進行Lv1이상
	SIF ABL:새드끼 >= 2 && CFLAG:461 >= 1
		PRINTFORML 「それとも……ボクにされたいのかニャ～？」
	PRINTFORMW 「カクゴするにゃ～♪」
	RETURN 1
;굴복각인Lv3
ELSEIF MARK:굴복각인 == 3 && TALENT:연모 == 0
	DRAWLINE
	P = RAND:15
	;새드ッ気Lv3이상, 역조교進行Lv2이상, ５分の１, または両方Lv4이상で３分の１
	IF ((ABL:새드끼 >= 3 && CFLAG:461 >= 2) && P < 3) || ((ABL:새드끼 >= 4 && CFLAG:461 >= 4) && P < 5)
		CALL SELECT_6_1
	;ねこみみめいど
	ELSEIF CHECK_SHIRT("메이드복")
		PRINTFORML 「にゃは, %CNAME("二人称")%, いらっしゃいにゃ～」
		PRINTFORMW 「何をなさいますにゃ～？にゃはは♪」
	;자위중독Lv3이상で３分の１
	ELSEIF ABL:자위중독 >= 3 && RAND:3 == 0
		PRINTFORML 「にゃう…, はぁっ, うにゃっ, にゃ…ふにゃぁ…」
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 一人でオナニーに耽っていた
		PRINTFORML ドアをノックすると, %조사처리(CALLNAME:TARGET,"는")% ゆっくりこちらに振り向いた
		PRINTFORMW 「……にゃはぁ, 来たにゃ……はやくするにゃ…」
	ELSE
		IF RAND:2 == 0
			PRINTFORML 「にゃはは, いらっしゃいにゃ～」
			PRINTFORML 「にゃふ……キモチイイことするにゃ…♪」
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の姿を認めると, こちらを熱く見つめてきた
		ELSE
			PRINTFORML 「…にゃは, 今일はナニするにゃ？」
			PRINTFORML 「……それとも……, して欲しいにゃ……？」
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 見つめ, 蕩けた笑みを浮かべた
		ENDIF
	ENDIF
	RETURN 1
;굴복각인Lv3+연모
ELSEIF TALENT:연모
	DRAWLINE
	P = RAND:4
	;새드ッ気Lv3이상, 역조교進行Lv2이상, ４分の１, または両方Lv4이상で２分の１
	IF ((ABL:새드끼 >= 3 && CFLAG:461 >= 2) && P == 0) || ((ABL:새드끼 >= 4 && CFLAG:461 >= 4) && P < 2)
		CALL SELECT_6_1
	;자위중독Lv3이상で３分の１
	ELSEIF ABL:자위중독 >= 3 && RAND:3 == 0
		PRINTFORML 「にゃ…, あっ, ……あ, にゃんっ」
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 一人でオナニーに耽っていた
		;ねこみみめいど
		IF CHECK_SHIRT("메이드복")
			PRINTFORML %조사처리(CALLNAME:MASTER,"가")% 部屋に入ると, 손を止めて얼굴を伏せた
			PRINTFORMW 「……にゃう, えっちなメイドに……お仕置き, してください…にゃ♪」
		ELSE
			PRINTFORML が, %조사처리(CALLNAME:MASTER,"가")% 部屋に入ると, 素早く飛びついてきた
			PRINTFORMW 「……にゃは, ボク…寂しかったにゃあ……」
		ENDIF
	ELSEIF RAND:3 == 0
		PRINTFORML 「ふにゃ～, 来てくれたにゃ～」
		PRINTFORML 「いっしょにキモチよくなるにゃ～♪」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")%, %CALLNAME:MASTER%の体に両손を回し, 抱きしめてきた
	ELSEIF RAND:2 == 0
		PRINTFORML 「うにゃ～っ, ちゅ～っ♪」
		PRINTFORML %조사처리(CALLNAME:MASTER,"가")% 部屋に入ると, %조사처리(CALLNAME:TARGET,"는")% 突然키스してきた
		PRINTFORMW 「にゃはは～, ビックリしたかにゃ～？」
	ELSE
		PRINTFORMW 「く～………にゃ……ぅ……」
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 眠っていたが, %조사처리(CALLNAME:MASTER,"가")% 部屋に入ると気付いて起きたようだ
		;ねこみみめいど
		IF CHECK_SHIRT("메이드복")
			PRINTFORMW 「ふにゃ…おはよーございますー……？」
		ELSE
			PRINTFORMW 「ふにゃ…おはよー……？」
		ENDIF
		PRINTFORMW %조사처리(CALLNAME:MASTER,"는")% 苦笑しつつ, %조사처리(CALLNAME:TARGET,"를")% 抱きしめた
	ENDIF
	RETURN 1
ENDIF
RETURN 1

;いきなり押し倒され？
@SELECT_6_1
	PRINTFORML %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%に近寄ると, 妖艶な笑みを浮かべ, 見上げながら尋ねてきた
	;ねこみみめいど
	IF CHECK_SHIRT("메이드복")
		PRINTFORML 「にゃはは, %CNAME("二人称")%…, 犯して欲しいニャ～？」
	ELSE
		PRINTFORML 「にゃは, 犯して欲しいニャ～？」
	ENDIF
PRINTFORML [0]はい
PRINTFORML [1]いいえ
CALL INPUT_SELECT, 2
IF RESULT == 0
	PRINTFORMW 「にゃはは…犯してアゲルにゃ～♪」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기た! 
	TEQUIP:넘어뜨리기 = 1
	IF CFLAG:461 < 4
		CALL K6_UP_F461
;		PRINTFORMW %CALLNAME:MASTER%の마조끼が{ABL:MASTER:마조끼}になった
	ENDIF	
ELSEIF RESULT == 1
	;새드ッ気Lv4이상, 역조교進行Lv4이상, ３分の２
	IF ((ABL:새드끼 >= 4 && CFLAG:461 >= 4) && RAND:3 > 0) || CFLAG:461 >= 6
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% 嗜虐的な笑みを浮かべながら%조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기, 上から見下ろしてきた
		PRINTFORMW 「にゃはは…嘘ついてもわかるニャ♪」
		;めいどか본디지
		IF CHECK_SHIRT("메이드복") || CHECK_BODYS("본디지")
			PRINTFORMW 「おしおきにゃ～♪」
		ELSE
			PRINTFORMW 「ホントはボクに犯して欲しいにゃ～♪」
		ENDIF
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"를")% 넘어뜨리기た! 
		TEQUIP:넘어뜨리기 = 1
	ELSE
		PRINTFORMW 「にゃう……ほんとに～？」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 残念そうに言った
	ENDIF
ENDIF

RETURN 1

;-------------------------------------------------
;EVENTEND関係
;조교종료時のセリフ CFLAG 211～220を使用
;-------------------------------------------------
@EVENTEND
SIF NO:TARGET != 6
	RETURN 1
SIF CFLAG:209 != 1
	RETURN 1

;-------------------------------------------------
;※정력限界値を超えた場合の終了구상　CFLAG451
;정력0でこちらがダウンしたときはCFLAG451で分岐
;-------------------------------------------------
;조교強制終了時の구상
IF STATE("撃沈", MASTER)
	DRAWLINE
	;반발각인LV3
	IF MARK:반발각인 == 3
		PRINTFORMW 
	;연모
	ELSEIF TALENT:연모
		PRINTFORMW 
	;その他
	ELSE
		PRINTFORMW 
	ENDIF
	RETURN 1
;반발각인Lv3
ELSEIF MARK:반발각인 == 3
	;반발각인Lv3（一回のみ）
	DRAWLINE
	IF CFLAG:220 == 0
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% %조사처리(CALLNAME:MASTER,"가")% 存在しないかのように, 着衣を整えた
		PRINTFORML 「……………………」
		PRINTFORMW 部屋を出ようとすると, 背中に凍りついてしまいそうな冷たい視線を感じたような気がした
		CFLAG:220 = 1
		RETURN 1
	;２回目以降, 체력500이상
	ELSEIF BASE:체력 >= 500
		PRINTFORML 「ふん…その程度にゃ？」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%に見向きもせず, 蔑むように言った
	;２回目以降, 체력500未満
	ELSE
		PRINTFORML 「はっ…はぁ……ふ…ぅ, …あはは…その程度にゃ」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%に見向きもせず, 蔑むように言った
	ENDIF
	RETURN 1
;촉수中, 음란か미약中か촉수중독Lv3이상, ２分の１
ELSEIF TEQUIP:촉수 && (TALENT:음란 || TEQUIP:미약 || EXP:촉수경험 >= 300) && RAND:2 == 0
	DRAWLINE
	PRINTFORML 「…ぅ………にゃ…あっ♪　………っ! ？　………ら……や…ぁ…♪」
	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 蠢く촉수に全身を絡めとられており 執拗な責めから逃れられないでいる…
	PRINTFORML 「ひ……にゃ♪　っん………ぅ……？　に………っ! 　…ぁ…………っ♪」
	PRINTFORMW %조사처리(CALLNAME:MASTER,"가")% 部屋を去ろうとした時も, %CALLNAME:TARGET%의 体は更に촉수の中に埋められていった……
;쓰러뜨려짐중, ２分の１
ELSEIF TEQUIP:넘어뜨리기 && RAND:2 == 0
	DRAWLINE
	;새드ッ気Lv3이상, 기교Lv3이상, 역조교進行Lv1이하
	IF ABL:새드끼 >= 3 && ABL:기교 >= 3 && CFLAG:461 <= 1 && ASSIPLAY == 0
		IF CFLAG:461 == 0
			PRINTFORML 「にゃは……虐められる快感, 刻んでアゲルにゃ♪」
		ELSE
			PRINTFORML 「にゃはは…, また…虐められたいのかニャ…？」
		ENDIF
		IF TALENT:MASTER:남자
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の페니스を両손で握ると, 猛烈な勢いでしごき始めた…
		ELSEIF TALENT:MASTER:후타나리 == 0
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の비렬を激しくかき混ぜながら, 유두や클리토리스に爪と牙で鈍い아픔と快感を与え続けた…
		ELSE
			PRINTFORML %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の페니스を片손で握ると, 猛烈な勢いでしごき始めた…
			PRINTFORMW さらにザラついた舌で비렬を激しくかき混ぜながら, 유두を爪で虐めている…
		ENDIF
		IF CFLAG:461 == 0
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の反応が快楽を伴ったものに変わったのを確認して, ようやく해방した……
			PRINTFORML %조사처리(CALLNAME:MASTER,"는")% 아픔とも快感ともつかない감각の中で, 虐められる快楽に目覚めた
		ELSE
			PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の声が快楽を伴ったものに変わったのを確認して, ようやく해방した……
			PRINTFORML %조사처리(CALLNAME:MASTER,"는")% %CALLNAME:TARGET%に, 被虐の快楽を教え込まれた…
		ENDIF
		CFLAG:461 += 1
;		PRINTFORMW %CALLNAME:MASTER%の마조끼が{ABL:MASTER:마조끼}になった
	;체력500이상
	ELSEIF BASE:체력 >= 500
		PRINTFORML 「にゃう……もう終わりにゃ？」
		PRINTFORMW 「仕方ないニャ……また今度, もっと犯してあげるニャ♪」
	ELSE
		PRINTFORML 「は…っ, は…ふう……気持ち…よかったニャ？」
		PRINTFORMW 「犯されてる時の声…, カワイかったにゃ…♪」
	ENDIF
;연모＋만족
ELSEIF TALENT:연모 && CFLAG:99
	DRAWLINE
	IF RAND:2 == 0
		PRINTFORML 「にゃ…はぁ…, ふにゃあ……, つかれたニャ……おやすみにゃ～」
		PRINTFORML %조사처리(CALLNAME:TARGET,"는")% %CALLNAME:MASTER%の腕を抱きこむとそれを枕に幸せそうに寝息を立て始めた
		PRINTFORMW 「くー……すぅ…」
	ELSE
		PRINTFORML 「ふー…にゅう…, 最後に…키스だけ, いいかにゃ…？」
		PRINTFORMW %조사처리(CALLNAME:MASTER,"가")% うなずくと, %조사처리(CALLNAME:TARGET,"는")% 키스をしてきた
		PRINTFORML 「ちゅ…んっ…, ちゅぷ……, ふぅ…ふにゃあ…, ありがとうにゃ…♪」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% にっこりと微笑み, %조사처리(CALLNAME:MASTER,"를")% 見送った
	ENDIF
	RETURN 1
;연모
ELSEIF TALENT:연모
	DRAWLINE
	PRINTFORML 「にゃう～……もういっちゃうにゃ～？」
	PRINTFORML 「うにゃ, 次はもっと激しくするにゃ……覚悟しておくにゃ～♪」
	PRINTFORMW  %조사처리(CALLNAME:TARGET,"는")% 손を振り, %조사처리(CALLNAME:MASTER,"를")% 見送った
	RETURN 1
;굴복각인Lv3
ELSEIF MARK:굴복각인 == 3
	DRAWLINE
	IF RAND:2 == 0
		PRINTFORML 「ふにゃ…あっ………」
		PRINTFORML 「キモチ……よかったにゃ……」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% 宙を見上げ, 惚けた얼굴でぼーっとしている
	ELSE
		PRINTFORML 조교を終えても%CALLNAME:TARGET%의 情欲は満たされず, 자위に耽っている
		PRINTFORML 「ふ…, にゃっ, にゃはぁ……うにゃ, ひあっ」
		PRINTFORMW %CALLNAME:TARGET%의 官能的な声を背に, %조사처리(CALLNAME:MASTER,"는")% 部屋を出た
	ENDIF
	RETURN 1
;굴복각인Lv2이하
ELSEIF MARK:굴복각인 < 3
	DRAWLINE
	;체력500이상
	IF BASE:체력 >= 500
	 	PRINTFORML 「ふにゅ…, もう終わりかニャ～？」
	 	PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% こちらを見ながらシッポを振っている
	;체력500未満
	ELSE
		PRINTFORML 「ふ…にゅう, ボク, つかれたにゃ…おやすみにゃ」
		PRINTFORMW %조사처리(CALLNAME:TARGET,"는")% そう言うと, 床に丸くなり, 寝てしまった
	ENDIF
	RETURN 1
ENDIF
